<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_stock_request">
            <field name="name">Stock Request</field>
            <field name="code">stock.request</field>
            <field name="prefix">SR-%(range_y)s%(range_month)s-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="use_date_range"/>
            <field name="padding">4</field>
        </record>
     </data>
</odoo>

